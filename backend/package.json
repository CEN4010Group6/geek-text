{
  "name": "geek-text-backend",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "prisma db seed --preview-feature"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CEN4010Group6/geek-text.git"
  },
  "author": "Joseph R. Quinn <423821+quinnjr@users.noreply.github.com>",
  "contributors": [
    "Paulo Pinto",
    "Arturo Presa",
    "Nicholas Prieto",
    "John Quitto"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CEN4010Group6/geek-text/issues"
  },
  "homepage": "https://github.com/CEN4010Group6/geek-text#readme",
  "dependencies": {
    "@nestjs/common": "^7.6.11",
    "@nestjs/core": "^7.6.11",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.11",
    "@prisma/client": "^2.16.0",
    "argon2": "^0.27.1",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "immutable": "^4.0.0-rc.12",
    "ioredis": "^4.22.0",
    "passport": "^0.4.1",
    "prisma": "^2.16.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.6.11",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
